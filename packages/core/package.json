{
  "name": "@penrose/core",
  "version": "1.3.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "author": "Penrose Team (https://penrose.ink)",
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "rimraf build src/parser/SubstanceParser.ts src/parser/StyleParser.ts src/parser/DomainParser.ts",
    "build:parsers": "nearleyc src/parser/Domain.ne > src/parser/DomainParser.ts && nearleyc src/parser/Substance.ne > src/parser/SubstanceParser.ts && nearleyc src/parser/Style.ne > src/parser/StyleParser.ts",
    "prebuild": "yarn build:parsers",
    "build": "vite build",
    "postbuild": "yarn build-decls",
    "prestart": "yarn build:parsers",
    "start": "vite",
    "prewatch": "yarn build:parsers",
    "watch": "vite",
    "pretest": "yarn build:parsers",
    "test": "jest --watchAll=false",
    "predocs": "yarn build:parsers",
    "test:watch": "jest --watchAll",
    "build-decls": "tsc --declaration --outDir dist && tsc-alias -p tsconfig.json",
    "docs": "typedoc --plugin none --out docs",
    "docs:md": "typedoc --plugin typedoc-plugin-markdown --out docs/md",
    "coverage": "yarn test --coverage --watchAll=false",
    "lint": "eslint --ext js,ts,tsx src",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@datastructures-js/heap": "^3.2.0",
    "@datastructures-js/queue": "^4.1.3",
    "consola": "^2.15.2",
    "fast-memoize": "^2.5.2",
    "graphlib": "^2.1.8",
    "heap-js": "^2.1.6",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.15",
    "mathjax-full": "^3.0.1",
    "ml-matrix": "^6.10.0",
    "moo": "^0.5.1",
    "nearley": "^2.20.1",
    "poly-partition": "^1.0.2",
    "recursive-diff": "^1.0.8",
    "rfdc": "^1.2.0",
    "seedrandom": "^3.0.5",
    "true-myth": "^4.1.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@penrose/examples": "^1.3.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@types/graphlib": "^2.1.7",
    "@types/jest": "^27.4.1",
    "@types/jscodeshift": "^0.11.0",
    "@types/lodash": "^4.14.149",
    "@types/moo": "^0.5.3",
    "@types/nearley": "^2.11.1",
    "@types/node": "^10.17.21",
    "@types/seedrandom": "^2.4.28",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "esbuild-visualizer": "^0.3.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.3",
    "eslint-plugin-react": "^7.28.0",
    "estrella": "^1.4.1",
    "jest": "^27.5.1",
    "jest-junit": "^12.0.0",
    "jscodeshift": "^0.11.0",
    "rollup": "^2.72.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "ts-jest": "^27.1.3",
    "tsc-alias": "^1.6.7",
    "tslib": "^2.1.0",
    "typedoc": "^0.22.11",
    "typedoc-plugin-markdown": "^3.11.13",
    "typescript": "^4.1.3",
    "vite-tsconfig-paths": "^3.4.1"
  }
}
